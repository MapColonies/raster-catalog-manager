{
  "name": "raster-catalog-manager",
  "version": "2.1.1",
  "description": "Service for inserting records into raster-catalog-manager",
  "main": "./src/index.ts",
  "scripts": {
    "test:unit": "jest --config=./tests/configurations/unit/jest.config.js",
    "test:integration": "jest --config=./tests/configurations/integration/jest.config.js",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "prelint:fix": "npm run format:fix && npm run generate-record-entity",
    "prelint": "npm run format && npm run generate-record-entity",
    "lint:openapi": "openapi lint ./openapi3.yaml",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "release": "standard-version",
    "pretest": "npm run generate-record-entity",
    "test": "npm run test:unit && npm run test:integration",
    "prebuild": "npm run clean && npm run generate-record-entity",
    "build": "tsc --project tsconfig.build.json && npm run bundleOpenApi && npm run assets:copy",
    "start": "npm run build && cd dist && node ./index.js",
    "assets:copy": "copyfiles -f ./config/* ./dist/config && copyfiles -f ./bundledApi.yaml ./dist/ && copyfiles ./package.json dist",
    "copySchema": "copyfiles -u 5 \"./node_modules/@map-colonies/mc-model-types/Schema/yaml/**/*\" ./Schema",
    "clean": "rimraf dist src/DAL/entity/generated.ts bundledApi.yaml",
    "install": "npx husky install",
    "generate-record-entity": "code-generator -p raster -gt orm -tf src/DAL/entity/generated.ts",
    "pretest:integration": "npm run bundleOpenApi",
    "bundleOpenApi": "openapi bundle --output ./bundledApi.yaml --ext yaml ./openapi3.yaml",
    "prebundleOpenApi": "npm run copySchema",
    "postbundleOpenApi": "rimraf Schema"
  },
  "directories": {
    "test": "tests"
  },
  "author": "MapColonies",
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@godaddy/terminus": "4.9.0",
    "@map-colonies/error-express-handler": "^2.1.0",
    "@map-colonies/error-types": "^1.1.0",
    "@map-colonies/express-access-log-middleware": "^0.0.3",
    "@map-colonies/js-logger": "^0.0.3",
    "@map-colonies/mc-model-types": "^12.1.0",
    "@map-colonies/openapi-express-viewer": "^2.0.1",
    "@map-colonies/read-pkg": "0.0.1",
    "@map-colonies/telemetry": "3.0.0",
    "@opentelemetry/api": "1.0.1",
    "@opentelemetry/api-metrics": "0.23.0",
    "@opentelemetry/instrumentation-express": "0.23.0",
    "@opentelemetry/instrumentation-http": "0.23.0",
    "@turf/bbox": "^6.3.0",
    "compression": "^1.7.4",
    "config": "^3.3.6",
    "express": "^4.17.1",
    "express-openapi-validator": "^4.13.2",
    "http-status-codes": "^2.1.4",
    "pg": "^8.6.0",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.1.6",
    "tsyringe": "^4.6.0",
    "typeorm": "^0.2.30",
    "wellknown": "^0.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "@map-colonies/code-generator": "^1.5.0",
    "@map-colonies/eslint-config": "^3.0.1",
    "@map-colonies/prettier-config": "0.0.1",
    "@map-colonies/standard-version-update-helm-version": "^2.0.1",
    "@map-colonies/standard-version-update-openapi3-version": "0.0.2",
    "@redocly/openapi-cli": "^1.0.0-beta.63",
    "@types/compression": "^1.7.2",
    "@types/config": "0.0.39",
    "@types/express": "^4.17.13",
    "@types/geojson": "^7946.0.7",
    "@types/jest": "^27.0.2",
    "@types/js-yaml": "^4.0.3",
    "@types/multer": "^1.4.7",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/wellknown": "^0.5.1",
    "commitlint": "^13.2.1",
    "copyfiles": "^2.4.1",
    "cz-conventional-changelog": "^3.0.1",
    "eslint": "^8.0.1",
    "husky": "^7.0.2",
    "jest": "^27.3.1",
    "jest-create-mock-instance": "^1.1.0",
    "jest-html-reporters": "^2.1.6",
    "jest-openapi": "^0.14.1",
    "js-yaml": "^4.1.0",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.1.1",
    "rimraf": "^3.0.2",
    "standard-version": "^9.3.2",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.7",
    "typescript": "^4.7.4"
  }
}
