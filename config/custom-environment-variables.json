{
  "openapiConfig": {
    "filePath": "OPENAPI_FILE_PATH"
  },
  "logger": {
    "level": "LOG_LEVEL"
  },
  "server": {
    "port": "SERVER_PORT",
    "request": {
      "payload": {
        "limit": "REQUEST_PAYLOAD_LIMIT"
      }
    }
  },
  "typeOrm": {
    "host": "DB_HOST",
    "port": "DB_PORT",
    "username": "DB_USER",
    "password": "DB_PASSWORD",
    "enableSslAuth": {
      "__name": "DB_SSL_ENABLE",
      "__format": "boolean"
    },
    "sslPaths": {
      "ca": "DB_SSL_CA",
      "key": "DB_SSL_KEY",
      "cert": "DB_SSL_CERT"
    },
    "database": "DB_NAME",
    "logging": "DB_LOGGING"
  }
}
