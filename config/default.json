{
  "openapiConfig": {
    "filePath": "./bundledApi.yaml",
    "basePath": "/docs",
    "rawPath": "/api",
    "uiPath": "/api"
  },
  "telemetry": {
    "logger": {
      "level": "info",
      "prettyPrint": false
    }
  },
  "server": {
    "port": "8080",
    "request": {
      "payload": {
        "limit": "10mb"
      }
    },
    "response": {
      "compression": {
        "enabled": true,
        "options": null
      }
    }
  },
  "typeOrm": {
    "type": "postgres",
    "host": "",
    "port": 5432,
    "username": "",
    "password": "",
    "enableSslAuth": false,
    "sslPaths": {
      "ca": "",
      "key": "",
      "cert": ""
    },
    "database": "raster",
    "schema": "public",
    "synchronize": false,
    "logging": false,
    "entities": [
      "**/DAL/entity/**/*.js"
    ],
    "migrations": [
      "**/DAL/migration/**/*.js",
      "**/DAL/migration/**/*.ts"
    ],
    "subscribers": [
      "**/DAL/subscriber/**/*.js"
    ],
    "cli": {
      "entitiesDir": "src/DAL/entity",
      "migrationsDir": "src/DAL/migration",
      "subscribersDir": "src/DAL/subscriber"
    }
  }
}
